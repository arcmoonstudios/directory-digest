#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/_Repos/directory-digest/node_modules/.pnpm/@vscode+vsce@3.6.1/node_modules/@vscode/vsce/node_modules:/mnt/c/_Repos/directory-digest/node_modules/.pnpm/@vscode+vsce@3.6.1/node_modules/@vscode/node_modules:/mnt/c/_Repos/directory-digest/node_modules/.pnpm/@vscode+vsce@3.6.1/node_modules:/mnt/c/_Repos/directory-digest/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/_Repos/directory-digest/node_modules/.pnpm/@vscode+vsce@3.6.1/node_modules/@vscode/vsce/node_modules:/mnt/c/_Repos/directory-digest/node_modules/.pnpm/@vscode+vsce@3.6.1/node_modules/@vscode/node_modules:/mnt/c/_Repos/directory-digest/node_modules/.pnpm/@vscode+vsce@3.6.1/node_modules:/mnt/c/_Repos/directory-digest/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../vsce" "$@"
else
  exec node  "$basedir/../../vsce" "$@"
fi
